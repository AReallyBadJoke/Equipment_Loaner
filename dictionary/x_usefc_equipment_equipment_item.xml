<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Equipment Item" max_length="40" name="x_usefc_equipment_equipment_item" type="collection">
        <element default="true" label="Available" max_length="40" name="available" type="boolean"/>
        <element choice="1" label="Category" mandatory="true" max_length="255" name="catagory" type="choice">
            <choice>
                <element label="Laptop" sequence="0" value="Laptop"/>
                <element label="Projector" sequence="1" value="Projector"/>
                <element label="Company Vehicle" sequence="2" value="Company Vehicle"/>
            </choice>
        </element>
        <element label="Description" max_length="255" name="description" type="string"/>
        <element default="2018-01-01" label="Due Date" max_length="40" name="due_date" type="glide_date_time"/>
        <element label="Loaner Department" max_length="32" name="group" reference="cmn_department" type="reference"/>
        <element label="Item Number/Designation" max_length="100" name="name" type="string"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <index name="index">
            <element name="group"/>
        </index>
    </element>
</database>
